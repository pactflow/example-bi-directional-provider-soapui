{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Collaborative contracts example, using Dredd and OAS to validate the provider API",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "concurrently -k -s \"first\" \"npm start\" \"sleep 3 && make test-soapui\"",
    "test:publish": "EXIT_CODE=0 make publish_provider_contract",
    "test:pact:consumer_change": "jest --testTimeout 30000 --testMatch \"**/*.consumerChange.pact.test.js\"",
    "test:pact:provider_change": "jest --testTimeout 30000 --testMatch \"**/*.providerChange.pact.test.js\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^12.1.1",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0"
  },
  "license": "ISC"
}
